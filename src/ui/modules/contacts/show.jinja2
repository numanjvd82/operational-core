{% extends "pages/contacts/index.jinja2" %}

{% block contact %}

<div class="card">
  <div class="card-body">
    <div>
      <b>ID:</b> {{ contact.id }}
    </div>
    <div>
      <b>Name:</b> {{ contact.name }}
    </div>
    <div>
      <b>Email:</b> {{ contact.email }}
    </div>
  </div>
</div>

{% endblock %}